





import pandas as pd





column_names = pd.Index(['apple', 'banana', 'mango', 'peach', 'kiwi'], name='fruit') # column names

row_names = pd.Index(['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i'], name='letters')  # row names

data = [
    [100, 110, 120, 130, 140],
    [200, 210, 220, 230, 240],
    [300, 310, 320, 330, 340],
    [400, 410, 420, 430, 440],
    [500, 510, 520, 530, 540],
    [600, 610, 620, 630, 640],
    [700, 710, 720, 730, 740],
    [800, 810, 820, 830, 840],
    [900, 910, 920, 930, 940],
]

df = pd.DataFrame(data, columns=column_names, index=row_names)
df








print(df['apple'])


print(df.apple)





print(df[['apple', 'mango']])








